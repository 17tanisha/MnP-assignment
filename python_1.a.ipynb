    "def F(n):\n",
    "    for i in range(1, n + 1):\n",
    "        string = \"\"\n",
    "        for j in range(1, i + 1):\n",
    "            string += chr(64 + j)\n",
    "        for j in range(i - 1, 0, -1):\n",
    "            string += chr(64 + j)\n",
    "        print(string.center(2 * n - 1, \"-\"))\n",
    "\n",
    "\n",
    "F(10)\n",
    "F(6)"
   ]
  }
